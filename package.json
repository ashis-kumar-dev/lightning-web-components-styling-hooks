{
  "name": "lightning-web-components-styling-hooks",
  "displayName": "Lightning Web Components Styling Hooks",
  "description": "Styling hooks available for standard lightning web components",
  "author": "Ashis Kumar Maharana",
  "publisher": "ashis-kumar-dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashis-kumar-dev/lightning-web-components-styling-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/ashis-kumar-dev/lightning-web-components-styling-hooks/issues"
  },
  "icon": "icon.jpg",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package -o ./bin/lightning-web-components-styling-hooks.vsix",
    "publish": "vsce publish",
    "scrape": "call .venv/Scripts/activate && python ./scripts/scraper.py && deactivate"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.3"
  }
}
